{
  "name": "madeitbelgium/wordpress-php-sdk",
  "version": "1.6.0",
  "description": "WordPress Laravel PHP SDK",
  "keywords": [
    "wordpress",
    "laravel",
    "php"
  ],
  "homepage": "https://www.madeit.be",
  "type": "library",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Tjebbe Lievens",
      "homepage": "https://www.madeit.be",
      "email": "tjebbe.lievens@madeit.be"
    }
  ],
  "require": {
    "illuminate/support": "*",
    "illuminate/container": "*",
    "guzzlehttp/guzzle": "6.*|7.*",
    "illuminate/validation": "*",
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*|6.*|8.*",
    "mockery/mockery": "0.9",
    "friendsofphp/php-cs-fixer": "2.11.*",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "MadeITBelgium\\WordPress\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MadeITBelgium\\WordPress\\WordPressServiceProvider"
      ],
      "aliases": {
        "WP": "MadeITBelgium\\WordPress\\WordPressFacade"
      }
    }
  }
}
